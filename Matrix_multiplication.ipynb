   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Directional vectors\n",
    "v1 = [1,2]\n",
    "v2 = [6,3]\n",
    "V = np.array(v1, v2)\n",
    "\n",
    "# Vector origin location\n",
    "origin = np.array([[0, 0],[0, 0]]) # origin point\n",
    "\n",
    "plt.quiver(*origin, V[:,0], V[:,1], color=['r','b'], scale=20)\n",
    "plt.show()"
   ]
